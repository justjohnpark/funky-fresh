  <% @pantry.items.order(expiration_date: :asc).each do |item| %>
    <tr class="item-row">
        <td><%= item.prototype.name %></td>
        <td><%= item.expiration_date %></td>
        <td><%= link_to image_tag('trash_can.png', :class => "trash_can"), user_pantry_item_path(@user, @pantry, item), remote: true, method: :delete, :class => "item-delete" %></td>
    </tr>
  <% end %>
